import java.io.IOException;
import java.util.Scanner;

public class Main {
 
    public static void main(String[] args) throws IOException {
        Scanner ler = new Scanner(System.in);
        int n = ler.nextInt();
        int nFatorial = 1;
        int i = 1;
        while(i <= n) {
             nFatorial = nFatorial * i;
            i++;
        }
        System.out.println(nFatorial);
 
    }
 
}
